{
  "name": "polythene-mithril-setup",
  "description": "Polythene for Mithril setup example",
  "version": "1.0.0-rc.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/polythene-mithril-setup.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "webpack-dev-server --config scripts/webpack.config.dev.js --port 3000",
    "dev:watch": "webpack --watch --config scripts/webpack.config.dev.js",
    "build": "webpack -p --config scripts/webpack.config.prod.js"
  },
  "dependencies": {
    "mithril": "1.1.5",
    "polythene-core": "^1.0.0-rc.8",
    "polythene-css": "^1.0.0-rc.8",
    "polythene-fastclick": "^1.0.0-rc.8",
    "polythene-mithril-dialog": "^1.0.0-rc.8",
    "polythene-mithril-raised-button": "^1.0.0-rc.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "npm-run-all": "^4.1.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
